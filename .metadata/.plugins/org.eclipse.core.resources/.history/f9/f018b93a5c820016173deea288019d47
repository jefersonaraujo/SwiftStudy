package br.edu.fa7.tarefas;

public class TesteEstudante {
	public static void main(String args[]){
		Estudante e = new Estudante();
		e.matricula=111;
		e.nome="Carlos";
		e.sexo='M';
		e.atribuirNota(1, 8.5);
		e.atribuirNota(2, 9.0);
		e.atribuirNota(3, 7.0);
		e.atribuirNota(4, 10.0);
		e.exibir();
		
		double nota1 = e.lerNota(1);
		
		System.out.println("nota1 = "+nota1);
	}
}
