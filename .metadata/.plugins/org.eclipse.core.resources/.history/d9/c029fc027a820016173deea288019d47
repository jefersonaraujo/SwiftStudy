package br.edu.fa7.tarefas;

import java.lang.reflect.*;

public class TesteFuncionarios {

	public static void main(String[] args) {
		Funcionario f = new Funcionario();
		f.recebeSalario();
		f = new Professor();
		f.recebeSalario();
		f = new Contador();
		f.contabilidade();
		f.recebeSalario();	
	}

}
